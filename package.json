{
  "name": "storybook-utils",
  "version": "2.0.1",
  "description": "Utilities for automated component documentation within Storybook",
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "docs",
    "*.js"
  ],
  "main": "./src/loader/index.js",
  "scripts": {
    "build": "yoshi build",
    "postbuild": "node ./build-tools/import-path --path src",
    "postinstall": "npm run build",
    "pretest": "build-storybook",
    "test": "npm run test:unit",
    "posttest": "npm run lint",
    "test:watch": "yoshi test --jest --watch",
    "test:unit": "yoshi test --jest",
    "lint": "yoshi lint",
    "start": "yoshi start --no-test & npm run storybook",
    "release": "yoshi release",
    "update-autodocs": "yarn add git+ssh://git@github.com:Evolent-Health/react-autodocs-utils.git#master"
  },
  "peerDependencies": {
    "semantic-ui-react": "^0.82.1"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "downshift": "^3.1.4",
    "github-markdown-css": "^2.9.0",
    "highlight.js": "9.14.2",
    "loader-utils": "^1.1.0",
    "prettier": "^1.16.2",
    "query-string": "^6.0.0",
    "react-autodocs-utils": "git+ssh://git@github.com:Evolent-Health/react-autodocs-utils.git#master",
    "react-collapse": "^4.0.3",
    "react-docgen": "^2.20.0",
    "react-element-to-jsx-string": "^14.0.2",
    "react-live": "^1.12.0",
    "react-markdown": "^4.0.6",
    "react-motion": "^0.5.2",
    "react-remarkable": "^1.1.3",
    "react-syntax-highlighter": "^9.0.0",
    "recast": "^0.14.7",
    "wix-ui-icons-common": "^2.0.24"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.17.3",
    "@storybook/addon-options": "4.0.0-alpha.9",
    "@storybook/react": "4.0.0-alpha.9",
    "@types/jest": "^23.3.8",
    "@types/react": "~16.8.1",
    "@types/react-dom": "~16.8.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "eslint-config-wix": "latest",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "latest",
    "lodash.kebabcase": "^4.1.1",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.8.6",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "wix-ui-test-utils": "^1.0.120",
    "yoshi": "^3.17.0"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "yoshi"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme-setup"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "yoshi": {
    "servers": {
      "cdn": {
        "port": 3201
      }
    }
  }
}
